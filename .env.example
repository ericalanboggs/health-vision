# OpenAI API Key for AI-enhanced features
# Get your API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# PostHog Analytics
# Get your project API key from: https://app.posthog.com/project/settings
VITE_POSTHOG_API_KEY=your_posthog_api_key_here
VITE_POSTHOG_HOST=https://us.i.posthog.com

# Supabase Backend
# Get your project URL and anon key from: https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Summit Pilot Configuration
# Set the start date for the pilot program (format: YYYY-MM-DD)
# All users will be on the same week schedule based on this date
VITE_PILOT_START_DATE=2025-01-12

# Twilio SMS Service
# Get these from: https://console.twilio.com/
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
VITE_TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# App URL for SMS links
VITE_APP_URL=https://your-app-domain.vercel.app

# Stripe Price IDs
# Get these from: https://dashboard.stripe.com/products
VITE_STRIPE_PRICE_CORE=
VITE_STRIPE_PRICE_PLUS=
VITE_STRIPE_PRICE_PREMIUM=
